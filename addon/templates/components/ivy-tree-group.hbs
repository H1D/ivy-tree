{{#each node.childNodes as |childNode|}}
  {{#component treeitemComponent
    activate=activate
    activeNode=activeNode
    collapse=collapse
    expand=expand
    groupComponent=groupComponent
    node=childNode
    tree=tree
    treeitemComponent=treeitemComponent as |descendantNode|}}
    {{yield descendantNode}}
  {{/component}}
{{/each}}
